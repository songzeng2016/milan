/* pages/supplyDetail/supplyDetail.wxss */
.de_Wrap{ position: absolute; left: 0; top: 0;  width: 100%; height: 100%;}
.de_Con{ background: #f4f4f4; height: 100%;  padding-bottom:100rpx; box-sizing: border-box; }
.de_Txt{ background: #fff; padding: 26rpx 20rpx 14rpx;}
.de_Txt_tit{ color: #333;font-size:32rpx; line-height: 40rpx; min-height: 40rpx; font-weight: bold;} 
.de_Txt_price{ color:#ff3333; font-size: 40rpx; font-weight: normal; margin: 10rpx 0 10rpx; height: 60rpx; line-height: 60rpx;}
.de_Txt_priceUnit{ font-size: 24rpx; }
.de_Txt_priceSym{ font-size: 20rpx; line-height: 70rpx;}
.de_Txt_sub{ height:40rpx ; line-height: 40rpx;  color: #999; font-size: 24rpx; padding: 20rpx 0 6rpx; border-top: 1px solid #ebebeb;}
.de_compCtn{ float: left; border-radius: 1px; border: 1px solid #ebebeb; color: #ff3333; font-size: 24rpx; height: 40rpx; line-height: 40rpx; padding: 0 12rpx; }
.de_compImg{ width: 22rpx; height: 24rpx; vertical-align: -2rpx;}
.de_comp_pv{ width: 26rpx; height: 19rpx;vertical-align:top; margin-left: 12rpx; position: relative; top: 12rpx;}
.de_Txt_dis{ display: flex; display: -webkit-flex; padding-bottom: 27rpx; align-items: center;}
.de_Txt_disL{ flex: 1; -webkit-flex:1; color: #333; font-size: 24rpx; word-break: break-all; text-align: justify; padding-right: 30rpx;line-height: 30rpx; min-height:60rpx; }
.de_Txt_disR{ width:80rpx; text-align: center; }
.de_Txt_disLtruck{ width:30rpx; height: 23rpx;margin-right: 10rpx; vertical-align: top; position: relative; top:3rpx;  }
.de_Txt_dis_pos{ width: 44rpx; height: 44rpx; }
.de_Txt_dis_txt{ font-size: 20rpx; color: #09bb07; display: block;margin:0 -5rpx;}

/*属性*/
.de_AttrTitArowR{  vertical-align: middle; width:14rpx; height: 24rpx; margin-top: -6rpx; margin-left:10rpx; }
.de_Attr{ margin-top: 20rpx; background: #fafafa; }
.de_AttrTit,.de_AttrItem{font-size: 26rpx;  }
.de_AttrTit{ height: 76rpx; line-height: 76rpx; }
.de_AttrTit{ color: #333;  padding: 0 20rpx; border-bottom: 1px solid #ebebeb; border-left:4rpx solid #09bb07 ; background: #fff;}

.de_AttrItem{ line-height: 36rpx; padding:26rpx 0; min-height: 36rpx;}
.de_AttrItem{ margin: 0 20rpx; color: #666564; border-top: 1px solid #ebebeb; margin-top: -1px; display: flex; display: -webkit-flex;}
.de_AttrItemL,.de_AttrItemR{word-break: break-all; text-align: justify; }
.de_AttrItemL{ flex: 1; margin-right: 10rpx;  color: #999; }
.de_AttrItemR{ flex: 2; color: #333;} 

/* */
.de_proTxt{ background: #fafafa; padding: 18rpx 20rpx; font-size: 26rpx; line-height: 40rpx; color: #333;}
/*用户评论 Begin*/
.de_comNoCon{ text-align: center; background: #fafafa; padding: 30rpx 0 30rpx;}
.de_comNoTxt{ color: #969996; font-size: 24rpx; line-height: 40rpx;}
.de_comNoBtn{ display: inline-block; font-size: 30rpx; height: 60rpx; line-height: 60rpx; margin-top: 20rpx;border: 1px solid #e7e7e7; background: #fff; padding: 0 25rpx;}
.de_green{ color: #09bb07;}
.de_AttrTitArowR{  vertical-align: middle; width:14rpx; height: 24rpx; margin-top: -6rpx;  }
.act_sort{color: #999;}
.act_UserDetWrap{ background: #fafafa;}
.act_UserDet{ padding: 15rpx 20rpx 22rpx; border-bottom: 1px solid #ebebeb;}
/*用户评论 End*/

.de_nearBy{ background: #fafafa;}
/* 供应列表 Begin */
.sup_item{ margin: 0 20rpx; padding: 30rpx 0; border-bottom: 1px solid #ebebeb;}
/* 供应列表 ：end */

/*采购列表  附近温馨提示 Begin*/
.de_nearEnd{ text-align: center;padding: 41rpx 0 94rpx;}
.de_nearEndTxt{ font-size: 24rpx; color: #999; line-height: 40rpx; height: 40rpx;}
.de_nearEndBtn{ width: 228rpx; height: 60rpx; line-height: 60rpx; border: 1px solid #ccc; border-radius: 4rpx; font-size: 24rpx; color: #09bb07; background: #fff;  margin: 34rpx auto 0;}
/*采购列表  附近温馨提示 :end*/


/* 底部 foot  Begin*/
.de_foot{ position: fixed; left: 0; bottom:0; width: 100%; height: 100rpx;  box-shadow: 0 -2px 0 0 rgba(0,0,0,0.05); background: #fff;font-size: 24rpx; color: #191a19; display: flex; display: -webkit-flex; z-index: 9; }
.de_ftItem{  flex: 1; text-align: center; }
.de_ftImgCon{ text-align: center;vertical-align: middle; height: 48rpx;  padding-top: 10rpx; }
.de_ftShareImg{ width: 36rpx; height: 34rpx;}
.de_ftcommentImg{ width: 38rpx; height: 40rpx;}
.de_ftTxt{ line-height: 30rpx;}
.de_ftItemTel{ flex: 2; background: #09bb07; height: 100rpx; color: #fff; text-align: center;}
.de_ftItemTelCon{ display: inline-block;}
.de_ftItemTelL{  width: 76rpx; height: 100rpx; float:left; }
.de_ftItemTelR{ float:left;   }
.de_ftItemTelL_pro{ margin-top: 10rpx; width:38rpx; height: 43rpx; }
.de_ftItemTelL_name{ height: 32rpx; overflow: hidden; line-height:32rpx;   margin: 0 auto;}
.de_ftItemTelR{margin-left: 6rpx; font-size: 30rpx; line-height: 100rpx; text-align: left;  }
.de_ftGrayAct{background: #f4f4f4;}
.de_ftGreenAct{background: #179b16;}
/* 底部 foot  End*/
.popAttr_Wrap{ background: rgba(0,0,0,0.5); position:fixed; left: 0; top: 0; width: 100%; height: 100%; z-index: 100;}
.popAttr_Con{ position: absolute; left: 0; bottom: 0; background: #fff; width: 100%;  }
.popAttr_Btn{height: 88rpx;line-height: 88rpx;background-color: #09bb07; color: #fff;position: absolute; bottom: 0rpx;width: 100%;text-align: center; font-size: 30rpx;}
.popAttr_BtnAct{background:#0ab007;}
.popAttr_Head{color: #333; font-size: 30rpx; text-align: center; line-height:88rpx; position: absolute; width: 100%; left: 0; top: 0; box-shadow: 0px 1px 1px rgba(0,0,0,0.1); }
.popAttr_Body{ height:750rpx;border-bottom: 1px solid #ebebeb; padding: 88rpx 0 88rpx; box-sizing: border-box;}

/* 评论 popUp Begin */
.ad_popWrap{ position: fixed; z-index: 100;  left: 0; top: 0;   width: 100%; height: 100%;}
.ad_popBg{ background: rgba(0, 0, 0, 0.5); position: absolute; left: 0; top: 0; width: 100%; height: 100%; z-index: 1;}
.ad_popCon{ position: absolute; left: 0; top: 30rpx; z-index: 2; background: #fff; width: 100%;background: #fff; border-radius: 9rpx;}
.ad_popHd{height: 76rpx; line-height: 76rpx; font-size: 32rpx;text-align: center; border-bottom: 1px solid #cdd1cd; padding: 0 20rpx;color: #333;}
.ad_popHdL,.ad_popHdR{font-size:28rpx ; height: 76rpx; line-height: 76rpx; color: #333;  padding: 0; background-color: transparent;}
.ad_popHdL{ float: left; }
.ad_popHdR{ float: right;}
.ad_popFt{ padding: 30rpx 20rpx 0;}
.ad_popArea{ width: 100%; height:276rpx;background: #fff;font-size:30rpx;padding: 8rpx; box-sizing: border-box; -webkit-box-sizing: border-box; line-height: 40rpx; color: #333; background: #f4f4f4;}
.ad_popCout{ color: #999; font-size: 24rpx; text-align: right; line-height: 58rpx; padding: 0 20rpx;}
.colorDanger{color: #ff3333;}
.ad_popError{ background: #ff3333; color: #fff; height: 58rpx; line-height: 58rpx; font-size: 24rpx; text-align: center;  position: absolute; left: 0; top: 0; width: 100%; z-index:3;}
/* 评论 popUp End */

/* 正在生成分享 弹窗 Begin */
.sha_popWrap{  position: fixed; z-index: 100; background: rgba(0,0,0,0.5); left: 0; top: 0; width: 100%; height: 100%;}
.sha_popCon{ position: absolute; left:50%; top: 50%; background: #fff; width:560rpx;margin-left: -280rpx; margin-top: -200rpx; border-radius: 8rpx;  text-align: center;}
.sha_popHead,sha_popCount{ height: 60rpx; line-height: 60rpx;}
.sha_popHead{ color: #333; font-size: 36rpx; padding-top: 45rpx; }
.sha_popCount{ color: #e64240; font-size: 40rpx;}
.sha_popTxt{ color: #999; padding: 10rpx 40rpx 30rpx; font-size: 30rpx;  line-height: 41rpx;}
.sha_popFt{ height:100rpx; line-height: 100rpx; border-top:1rpx solid #e9e9e9;font-size: 36rpx; display: flex; display: -webkit-flex; text-align: center;}
.sha_popBtnCancel{ color: #333; flex:1;}
.sha_popBtnOK{color: #00c200; flex: 1; border-left: 1px solid #e9e9e9;}
/* 正在生成分享 弹窗 End */
